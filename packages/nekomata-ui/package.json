		"@testing-library/svelte": "^3.1.3",
		"@types/jest": "^28.1.4",
		"@typescript-eslint/eslint-plugin": "^5.30.3",
		"@typescript-eslint/parser": "^5.30.3",
		"eslint": "^8.19.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
