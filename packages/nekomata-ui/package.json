{
  "name": "nekomata-ui",
  "license": "MIT",
  "version": "0.2.5",
  "svelte": "dist/index.js",
  "module": "dist/index.mjs",
  "main": "dist/index.umd.js",
  "types": "dist/ts/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "validate": "svelte-check",
    "test": "jest src",
    "test:watch": "npm run test -- --watch",
    "coverage": "jest src --collectCoverage"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "8.3.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^2.0.1",
    "babel-jest": "^27.0.6",
    "eslint-plugin-jest-dom": "^3.9.0",
    "fs-extra": "10.0.0",
    "glob": "7.2.0",
    "jest": "^27.4.4",
    "rollup": "^2.56.2",
    "rollup-plugin-execute": "1.1.1",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-svelte": "^6.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.42.4",
    "svelte-check": "^2.2.4",
    "svelte-jester": "^2.1.5",
    "svelte-preprocess": "^4.7.4",
    "tar": "^6.1.9",
    "ts-jest": "^27.0.4",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "y18n": ">=5.0.8"
  },
  "keywords": [
    "svelte",
    "nekomata-ui",
    "typescript",
    "ui",
    "ui-components",
    "components",
    "svelte3"
  ],
  "author": "Open Anthro Projects",
  "files": [
    "src",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/open-anthro-projects/nekomata-ui"
  },
  "homepage": "https://open-anthro-projects.github.io/nekomata-ui/",
  "bugs": {
    "url": "https://github.com/open-anthro-projects/nekomata-ui/issues"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  }
}
