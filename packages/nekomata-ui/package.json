{
	"name": "nekomata-ui",
	"license": "MIT",
	"version": "0.3.0",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"coverage": "vitest run --coverage"
	},
	"devDependencies": {
		"@babel/core": "^7.20.12",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-typescript": "^7.18.6",
		"@linaria/babel-preset": "^4.4.1",
		"@linaria/core": "^4.2.6",
		"@linaria/vite": "^4.2.7",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@sveltejs/package": "^1.0.0",
		"@sveltejs/vite-plugin-svelte": "^2.0.2",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^3.2.2",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@vitest/coverage-c8": "^0.28.4",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"global-jsdom": "^8.7.0",
		"jsdom": "^21.1.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-preprocess": "^5.0.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"keywords": [
		"svelte",
		"nekomata-ui",
		"typescript",
		"ui",
		"library",
		"ui-components",
		"components",
		"svelte3"
	],
	"author": "Open Anthro Projects",
	"repository": {
		"type": "git",
		"url": "https://github.com/open-anthro-projects/nekomata-ui"
	},
	"homepage": "https://open-anthro-projects.github.io/nekomata-ui/",
	"bugs": {
		"url": "https://github.com/open-anthro-projects/nekomata-ui/issues"
	},
	"type": "module"
}
