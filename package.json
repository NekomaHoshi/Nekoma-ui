  "devDependencies": {
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "gh-pages": "^3.2.3",
    "nekomata-ui": "0.2.11",
    "prettier": "~2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.3",
    "svelte-preprocess": "^4.10.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "ansi-regex": ">=6.0.1"
  },
  "type": "module"
}
