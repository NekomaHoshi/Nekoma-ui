  "devDependencies": {
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "gh-pages": "^4.0.0",
    "nekomata-ui": "0.3.0",
    "prettier": "~2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "ansi-regex": ">=6.0.1"
  },
  "type": "module"
}
